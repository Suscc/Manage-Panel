.c-pageNum {
    &_last {
        .i-gArrow-left;
        cursor: pointer;
    }
    &_main {
        @spacLR: 30px;

        .inline-block;
        margin-left: @spacLR; margin-right: @spacLR;
    }
    &_item {
        cursor: pointer;
        .inline-block;

        & + & {
            margin-left: 20px;
        }
        &-active {
            color: @mainColor;
        }
    }
    &_next {
        .i-gArrow-right;
        cursor: pointer;
    }
}

.c-table {
    width: 100%;
    text-align: center;
    border-collapse:collapse;
    border-top-left-radius: @radius;
    border-top-right-radius: @radius;
    overflow: hidden;

    > thead {
        color: @fontWhite;
        background-color: @mainColor;

        > tr {
            width: 100%;
            height: 40px;
            table-layout: fixed;
            display: table;
        }
    }

    > tbody {
        background-color: #fff;
        overflow: auto;
        overflow-x: hidden;
        display: block;
        position: relative;

        .a {
            position: absolute;
            top: 0;right: 0;
            height: 50px;
            width: 20px;
            background-color: red;
            z-index: 999;
        }

        > tr {
            width: 100%;
            display: table;
            table-layout: fixed;
        }

        > tr > td {
            @spaTB: 8px;

            padding-top: @spaTB; padding-bottom: @spaTB;
        }
    }

    &_multiple {
        .inline-block;
        text-align: left;
    }
}

.table {
    display: table;
    width: 100%;
}
.row {
    display: table-row;
}
.rowGro {
    display: table-row-group;
}
.col {
    display: table-cell;
